{
  "name": "json-len",
  "displayName": "json-len",
  "description": "Show length of json array.",
  "version": "0.0.2",
  "icon": "images/icon.png",
  "repository": {
    "type": "git",
    "url": "https://github.com/kongtaoxing/json-len.git"
  },
  "engines": {
    "vscode": "^1.96.2"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onLanguage:json"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "configuration": {
      "title": "JSON Length Viewer",
      "properties": {
        "jsonLen.maxFileSize": {
          "type": "number",
          "default": 10,
          "markdownDescription": "%config.maxFileSize.description%"
        },
        "jsonLen.fileSizeUnit": {
          "type": "string",
          "enum": ["KB", "MB", "GB"],
          "default": "MB",
          "markdownDescription": "%config.fileSizeUnit.description%"
        }
      }
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/vscode": "^1.96.2",
    "@types/mocha": "^10.0.10",
    "@types/node": "20.x",
    "@typescript-eslint/eslint-plugin": "^8.25.0",
    "@typescript-eslint/parser": "^8.25.0",
    "eslint": "^9.21.0",
    "typescript": "^5.7.3",
    "@vscode/test-cli": "^0.0.10",
    "@vscode/test-electron": "^2.4.1"
  },
  "publisher": "kongtaoxing"
}
